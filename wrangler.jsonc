{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "uilab-website",
  "main": "./dist/index.js",
  "compatibility_date": "2026-02-18",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "assets": {
    "directory": "./dist"
  },
  "r2_buckets": [
    {
      "binding": "BUCKET",
      "bucket_name": "uilab-media"
    }
  ],
  "d1_databases": [
    {
      // wrangler d1 create uilab-content で取得したIDをここに設定
      "binding": "DB",
      "database_name": "uilab-content",
      "database_id": ""
    }
  ]
}
